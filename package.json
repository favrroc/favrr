{
  "name": "market-favrr-frontend",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "webpack serve --mode development --https --key ./webpack/cert/localhost-key.pem --cert ./webpack/cert/localhost.pem",
    "start-windows": "webpack serve --mode development --https --key ./webpack/cert/windows/cert.key --cert ./webpack/cert/windows/cert.crt",
    "prebuild": "npm run cleanup",
    "cleanup": "rm -rf build; npm run messages",
    "build": "webpack --mode production",
    "build:dev": "webpack --mode development",
    "postbuild": "cp src/assets/template/sitemap.xml build/static/sitemap.xml",
    "lint": "eslint src/**/*.{tsx,ts}",
    "lint-fix": "eslint src/**/*.{tsx,ts} --fix",
    "format": "prettier --write \"**/*.{ts,tsx,scss}\"",
    "messages:extract": "formatjs extract \"src/**/*.{tsx,ts}\" --out-file generated/messages.json --ignore \"src/**/*.d.{tsx,ts}\"",
    "messages:compile": "formatjs compile generated/messages.json --out-file src/localization/en.json",
    "generate-graphql-types": "dotenv -- graphql-codegen --config codegen-graphql.yml",
    "generate-subgraph-types": "dotenv -- graphql-codegen --config codegen-subgraph.yml",
    "messages": "npm run messages:extract; npm run  messages:compile",
    "deploy": "npm run build && npx serverless deploy",
    "test": "echo \"Error: no test specified\" && exit 1",
    "husky-install": "husky install"
  },
  "repository": {
